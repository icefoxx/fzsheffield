#!/usr/bin/python

class Sheep:
    def __init__(self,name):
        self.name = name
        self.breed = None
        self.numreads = None
        self.regions = None

class SNP:
    def __init__(self,position,region,mutant_base,ref_base):
        self.position = position
        self.region = region
        self.mutant_base = mutant_base
        self.ref_base = ref_base
        self.basecount = {A:0,C:0,G:0,T:0)

